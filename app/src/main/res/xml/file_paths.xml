<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- External storage for pictures taken by camera -->
    <external-files-path
        name="external_files"
        path="Pictures/" />

    <!-- Cache directory for temporary files -->
    <cache-path
        name="cache"
        path="." />

    <!-- External cache directory -->
    <external-cache-path
        name="external_cache"
        path="." />

    <!-- Files directory -->
    <files-path
        name="files"
        path="." />
</paths>